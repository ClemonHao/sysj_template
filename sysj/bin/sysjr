#!/bin/bash

PRG="$0"
# Need this for relative symlinks.
while [ -h "$PRG" ] ; do
    ls=`ls -ld "$PRG"`
    link=`expr "$ls" : '.*-> \(.*\)$'`
    if expr "$link" : '/.*' > /dev/null; then
        PRG="$link"
    else
        PRG=`dirname "$PRG"`"/$link"
    fi
done
SAVED="`pwd`"
cd "`dirname \"$PRG\"`/.." >&-
APP_HOME="`pwd -P`"
cd "$SAVED" >&-

source $APP_HOME/bin/init

#CMD="$JAVACMD -XstartOnFirstThread ${JVM_OPTS[@]} -classpath $CLASSPATH com.systemj.SystemJRunner $PARAM"
CMD="$JAVACMD ${JVM_OPTS[@]} -classpath $CLASSPATH com.systemj.SystemJRunner $PARAM"

exec $CMD
